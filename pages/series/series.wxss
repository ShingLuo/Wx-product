/* pages/seriesColligate/seriesColligate.wxss */
@import "../../components/series/truckImage/truckImage.wxss";
@import "../../components/series/modelTypeList/modelTypeList.wxss";
@import "../../components/series/otherConcerns/otherConcerns.wxss";
@import "../../components/dealer/dealer.wxss";
@import "../../components/selectLocation/selectLocation.wxss";
page{
  height:100%;
  padding-bottom:100rpx; 
  box-sizing: border-box;
  background-color:#f5f5f5;
}
.container{
  height:100%;
  background-color: #f5f5f5; 
  overflow: hidden;
}
.series-wrapper{
  height:100%;
}
.series-nav{
  display: flex;
  height: 92rpx;
  padding-top: 16rpx;
  background: #fff;
  justify-content: center;
  align-items: center;
  width: 100%;
}
.series-nav .nav-text{
  text-align: center;
  width: 187rpx;
  /* width: 250rpx;
  float: left; */
  font-size: 26rpx;
}
.nav-ico-pei:before{
  font-family: 'wxfont';
  display: block;
  content: '\e6fc';
  font-size: 38rpx;
  color: #999;
  padding-bottom: 10rpx;
}
.nav-ico-img:before{
  font-family: 'wxfont';
  display: block;
  content: '\e6fa';
  font-size: 38rpx;
  color: #999;
  padding-bottom: 10rpx;
}
.nav-ico-sell:before{
  font-family: 'wxfont';
  display: block;
  content: '\e6fd';
  font-size: 38rpx;
  color: #999;
  padding-bottom: 10rpx;
}
.nav-ico-com:before{
  font-family: 'wxfont';
  display: block;
  content: '\e712';
  font-size: 38rpx;
  color: #999;
  padding-bottom: 10rpx;
}
.summarize{
  width: 33.3%;
  text-align: center;
  font-size: 28rpx;
  border-bottom: 2rpx solid #586c94;
  color: #586c94;
}
.nav-selected{
  color: #586c94;
  border-bottom: 4rpx solid #586c94;
}
.footer-info{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height:100rpx;
  display: flex;
  flex-direction: row;
  text-align: center;
  z-index: 10;
  overflow: hidden;
}
.footer-nav{
  flex: 1;
  width:50%;
  border-top:1px solid #eee;
  background-color:#fff;
}
.footer-nav > text{
  display: inline-block;
  width: 50%;
  color: #586c94;
  font-size: 32rpx;
  text-align: center;
  line-height:100rpx;
}
.footer-price{
  flex:1;
  padding: 16rpx 0 13rpx 0;
  border-top:1px solid #f60;
  background-color: #f60;
}
.footer-price .foot-price-text{
  display: block;
  font-size: 32rpx;
  line-height:28rpx;
  color: #fff;
}
.footer-price .number{
  font-size: 24rpx;
  color: rgba(255,255,255,0.7);
}
.switch-location-buttons{
  display: flex;
  height:80rpx;
  flex-direction: row;
}
.switch-location-buttons > view{
  flex: 1;
  text-align: center;
  line-height: 80rpx;
  color:#333;
  font-size:32rpx;
}
.switch-location-buttons .ok{
    color:#448dea;
    font-weight: bold;
    border-left:1px solid #eee;
}
.compare-number{
    position: fixed;
    right:-140rpx;
    bottom: 270rpx;
    width: 140rpx;
    height: 70rpx;
    line-height: 70rpx;    
    text-align: center; 
    color:#fff;
    font-size:24rpx;
    transition: right .3s;
    border-radius: 35rpx 0 0 35rpx;
    background-color: rgba(21,113,229,.8);
}
.compare-visible{
  right:0;
}
.mask-wrapper{
  height:100%;
  width:100%;
}
.mask-content{
  height:125%;
}

/* 写点评 */
.comm-top-box{
  height: 96rpx;
  display: flex;
  justify-content: flex-start;
  padding: 0 32rpx;
  background: #fff;
  margin-top: 24rpx;
  border-bottom: 2rpx solid #e5e5e5;
}
.comm-num-box{
  flex: 1;
  font-size: 32rpx;
  color: #333;
  line-height: 96rpx; font-weight: bold;
}
.comm-top-btn{
  width: 144rpx;
  height: 64rpx;
  border-radius: 8rpx;
  background-color: #edf2fc;
  border: solid 2rpx #b9c5dd;
  font-size: 28rpx;
  color: #586c94;
  line-height: 64rpx;
  text-align: center;
  margin-top: 14rpx;
}
.comm-top-btn::before{
  content: '\e601';
  font-family: 'wxfont';
  padding-right: 10rpx;
}

.comm-item-box{
  background: #fff;
  padding-left: 112rpx;
  position: relative;
}
.comm-left-ioc,.comm-left-ioc image{
  width: 64rpx;
  height: 64rpx;
  border-radius: 64rpx;
  overflow: hidden;
  position: absolute;
  top: 20rpx; text-align: center;
  left: 32rpx;
  z-index: 1;
  background: #f6f6f6;
}
.comm-left-ioc image{
  top: 0; left: 0;
  z-index: 3;
}
.comm-left-ioc::before{
  content: '\e61a';
  font-family: 'wxfont';
  font-size: 20rpx;
  color: rgb(189, 188, 188);
}
.comm-rt-box{
  border-bottom: 2rpx solid #e3e3e3;
}
.com-str-nam{
  height: 80rpx;
  width: 100%;
}
.com-str-nam .name{
  float: left;
  width: 466rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #16181a; overflow: hidden;
}
.com-str-nam .start{
  float: right;
  width: 170rpx;
  height: 80rpx;
}
.com-str-nam .start text{
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  line-height: 80rpx;
  margin-right: 4rpx;
}
.com-str-nam .start text::before{
  content: '\e600';
  font-family: 'wxfont';
  color: #d8d7d7;
  font-size: 24rpx;
}
.com-str-nam .start .red-str::before{
  color: #f60;
}
.com-cont-txt{
  padding-right: 32rpx;
  max-height: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  line-height: 40rpx; color: #5c6066;
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 5;
overflow: hidden;
 word-break:break-all;
  word-wrap:break-word;
}
.com-txt-all{
  display: block;
  width: 100%;
  line-height: 40rpx;
  height: auto;
  word-break:break-all;
  word-wrap:break-word;
}
.com-fot-box{
  height: 76rpx;
}
.com-fot-box .time{
  float: left; width: 340rpx;
  height: 76rpx; line-height: 76rpx;
  font-size: 24rpx; color: #a1a9b3; overflow: hidden;
}
.com-fot-box .zan{
  padding-right: 32rpx;
  float: right;
  font-size: 24rpx;
  width: 240rpx;
  height: 76rpx; line-height: 76rpx; color: #ccc;
  text-align: right;
}
.com-fot-box .red{ color: #f60;}
@keyframes zanss{
  0%{transform: scale(1.2);}
  50%{transform: scale(0.8);}
  70%{transform: scale(1.1);}
  100%{transform: scale(1);}
}
.com-fot-box .zan::before{
  content: '\e866';
  font-family: 'wxfont';
  padding-right: 10rpx;
  display: inline-block;
}
.com-fot-box .red::before{animation:zanss 1s;}

.com-more-box{
  width: 100%;
  height: 240rpx;
  position: relative;
}
.mor-com-box{
  position: relative;
  height: 80rpx;
  border-top: 2rpx solid #e5e5e5;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  color: #999;
  background: #fff;
  top: -2rpx;
  z-index: 3;
}
.mor-com-box::after{
  content: '\e620';
  font-family: 'wxfont';
  
}
.kong{
  width: 100%;
  height:360rpx;
  margin: 0 auto;
  background:url(data:image/png;base64,) center -30rpx no-repeat #fff;
  background-size: 440rpx auto;
}
.kong::before{
  content: '还没有点评哦';
  display: block;
  font-size: 24rpx;
  color: rgb(168, 168, 168);
  text-align: center;
  padding-top: 300rpx;
}